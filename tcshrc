xmodmap -e 'clear Lock'
xmodmap ~/.Xmodmap
xset r rate 200 20	

"\e[5~": history-search-backward
"\e[6~": history-search-forward
bindkey "\e[5~" history-search-backward		#syntax for tcsh
bindkey "\e[6~" history-search-forward
bindkey -s "\e[25~" ""
